{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "better-blocks/read-time",
	"version": "0.1.0",
	"title": "Read Time",
	"category": "content",
	"description": "Display an estimated reading time for this post",
	"attributes": {
		"readTime": {
			"type": "number"
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "better-blocks",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css"
}
