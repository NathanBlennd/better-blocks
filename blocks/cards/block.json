{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "better-blocks/cards",
	"version": "0.1.0",
	"title": "Cards",
	"category": "content",
	"description": "A collection of cards in a card deck",
	"attributes": {
		"size": {
			"enum": [ "1", "2", "3", "4" ]
		},
		"type": {
			"type": "string"
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "better-blocks",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css"
}
