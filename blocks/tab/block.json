{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "better-blocks/tab",
	"version": "0.1.0",
	"title": "Tab",
	"category": "content",
	"description": "Used exclusively as part of a tabs group",
	"attributes": {
		"label":{
            "type": "string",
			"default": ""
        },
		"index":{
            "type": "number",
			"default": ""
        }
	},
	"parent": [ "better-blocks/tabs" ],
	"supports": {
		"html": false
	},
	"textdomain": "better-blocks",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css"
}
