{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "better-blocks/card",
	"version": "0.1.0",
	"title": "Card",
	"category": "content",
	"description": "An image and content in a card format",
	"attributes": {
		"heading": {
			"type": "string",
			"source": "html",
			"selector": ".card-heading"
		},
		"content": {
			"type": "string",
			"source": "html",
			"selector": ".card-content"
		},
		"imageUrl": {
            "type": "string"
        }
	},
	"parent": [ "better-blocks/cards" ],
	"supports": {
		"html": false
	},
	"textdomain": "better-blocks",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css"
}
