{
	"name": "better-blocks",
	"version": "0.1.0",
	"description": "Registers custom blocks.",
	"author": "Blennd Ninjas",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"build": "wp-scripts build",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"start": "wp-scripts start",
		"packages-update": "wp-scripts packages-update",
		"start:accordion": "wp-scripts start blocks/accordion/src/index.js --output-path=blocks/accordion/build/",
		"build:accordion": "wp-scripts build blocks/accordion/src/index.js --output-path=blocks/accordion/build/",
		"start:accordion-item": "wp-scripts start blocks/accordion-item/src/index.js --output-path=blocks/accordion-item/build/",
		"build:accordion-item": "wp-scripts build blocks/accordion-item/src/index.js --output-path=blocks/accordion-item/build/",
		"start:card": "wp-scripts start blocks/card/src/index.js --output-path=blocks/card/build/",
		"build:card": "wp-scripts build blocks/card/src/index.js --output-path=blocks/card/build/",
		"start:cards": "wp-scripts start blocks/cards/src/index.js --output-path=blocks/cards/build/",
		"build:cards": "wp-scripts build blocks/cards/src/index.js --output-path=blocks/cards/build/"
	},
	"dependencies": {
		"@wordpress/block-editor": "^9.5.0",
		"@wordpress/blocks": "^11.12.0",
		"@wordpress/i18n": "^4.13.0",
		"@wordpress/url": "^3.14.0",
		"md5": "^2.3.0",
		"typescript": "^4.6.4"
	},
	"devDependencies": {
		"@wordpress/scripts": "^23.5.0"
	}
}
